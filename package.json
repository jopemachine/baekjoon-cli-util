{
	"name": "baekjoon-cli-utils",
	"version": "0.1.1",
	"description": "Simple code runner and CLI tool for studying, testing and managing Baekjoon algorithm source codes efficiently",
	"main": "dist/index.js",
	"author": "jopemachine",
	"license": "MIT",
	"bin": {
		"baekjoon-cli": "dist/cli.js"
	},
	"repository": {
		"url": "https://github.com/jopemachine/baekjoon-cli-util"
	},
	"scripts": {
		"test": "ava",
		"build": "tsc",
		"lint": "xo --fix"
	},
	"files": [
		"dist",
		"runner-settings.json"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"type": "module",
	"keywords": [
		"algorithm",
		"baekjoon",
		"cli"
	],
	"dependencies": {
		"boxen": "^7.0.0",
		"chalk": "^5.0.0",
		"change-case": "^4.1.2",
		"cheerio": "^1.0.0-rc.10",
		"clipboardy": "^3.0.0",
		"conf": "^10.1.1",
		"del": "^6.1.1",
		"env-paths": "^3.0.0",
		"execa": "^6.1.0",
		"filenamify": "^5.1.1",
		"find-up": "^6.3.0",
		"first-run": "^3.0.0",
		"globby": "^13.1.1",
		"got": "^12.0.1",
		"html-to-text": "^8.2.0",
		"inquirer": "^8.2.0",
		"inquirer-autocomplete-prompt": "^2.0.0",
		"is-promise": "^4.0.0",
		"log-symbols": "^5.1.0",
		"nvexeca": "^8.1.0",
		"open": "^8.4.0",
		"ora": "^6.1.1",
		"outdent": "^0.8.0",
		"parse-json": "^6.0.2",
		"path-exists": "^5.0.0",
		"tempy": "^3.0.0",
		"unused-filename": "^4.0.0"
	},
	"devDependencies": {
		"ava": "^4.3.0",
		"typescript": "^4.7.4",
		"xo": "^0.48.0"
	},
	"xo": {
		"rules": {
			"@typescript-eslint/naming-convention": "off",
			"@typescript-eslint/no-empty-interface": "off",
			"@typescript-eslint/no-implicit-any-catch": "off",
			"@typescript-eslint/no-unsafe-assignment": "off",
			"@typescript-eslint/no-unsafe-call": "off",
			"@typescript-eslint/no-unsafe-return": "off",
			"@typescript-eslint/restrict-template-expressions": "off",
			"ava/no-import-test-files": "off",
			"unicorn/no-abusive-eslint-disable": "off",
			"unicorn/no-process-exit": "off",
			"unicorn/no-await-expression-member": "off"
		}
	},
	"ava": {
		"files": [
			"test/**"
		]
	}
}
